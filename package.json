{
  "name": "nestjs-prisma-client-starter",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Marc Stammerjohann",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fivethree-team/nestjs-prisma-client-example.git"
  },
  "bugs": {
    "url": "https://github.com/fivethree-team/nestjs-prisma-client-example/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate:save": "npx prisma2 migrate save --experimental",
    "migrate:up": "npx prisma2 migrate up --experimental",
    "prisma:studio": "npx prisma2 studio --experimental",
    "prisma:generate": "npx prisma2 generate",
    "prisma:generate:watch": "npx prisma2 generate --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d"
  },
  "dependencies": {
    "@nestjs/common": "6.11.11",
    "@nestjs/config": "0.3.0",
    "@nestjs/core": "6.11.11",
    "@nestjs/graphql": "6.6.2",
    "@nestjs/passport": "6.2.0",
    "@nestjs/platform-express": "6.11.11",
    "@nestjs/swagger": "4.3.2",
    "@prisma/client": "2.0.0-preview024",
    "apollo-server-express": "2.11.0",
    "bcrypt": "5.0.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "jwks-rsa": "^1.7.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "swagger-ui-express": "4.1.3",
    "type-graphql": "0.17.6"
  },
  "devDependencies": {
    "@nestjs/cli": "6.14.2",
    "@nestjs/testing": "6.11.11",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.0.9",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "chance": "1.1.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "25.1.0",
    "prettier": "2.0.1",
    "prisma2": "^2.0.0-preview024",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.8.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
